<script type="text/ng-template" id="type_5_setter.html">
    <div ng-controller="WizardParamSetter5Controller">
        {% embed "LeapPanelBundle:TestWizard:type_base_setter.html.twig" %}
            {% trans_default_domain "TestWizard" %}
            {% block setter %}
                <select ng-model="output" ng-change="onPrimitiveValueChange(output)"
                        ng-disabled="!editable"
                        class='form-control'>
                    <option value="">{{ "none.choosen"|trans({},"panel") }}</option>
                    <option ng-repeat="template in viewTemplateCollectionService.collection | filter : {'archived':'!1'} | orderBy: 'name'" ng-value="template.name" ng-bind="template.name"></option>
                </select>
            {% endblock %}
        {% endembed %}
    </div>
</script>