{% trans_default_domain "TestWizard" %}
<div type="text/ng-template" id="param_definer_dialog.html">
    <div class="modal-body">
        <wizard-param-definer
                editable="isEditable()"
                param="param"
                wizard-object="wizardObject ? wizardObject : object"
                types="typesCollection"></wizard-param-definer>
    </div>
    <div class="modal-footer">
        <button class="btn btn-success" ng-click="change()">{{ 'form.button.save'|trans({},"panel") }}</button>
    </div>
</div>
